{
  "name": "@nucle-x/ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/lib/index.umd.cjs",
  "module": "./dist/lib/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/lib",
    "dist/types"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.umd.cjs"
    },
    "./css": {
      "import": "./dist/lib/style.css",
      "require": "./dist/lib/style.css"
    }
  },
  "scripts": {
    "types": "vue-tsc",
    "types:watch": "vue-tsc --watch",
    "build": "vue-tsc && vite build",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@nucle-x/config": "workspace:*",
    "@types/node": "^20.12.10",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.19",
    "flowbite": "^1.8.1",
    "fuse.js": "^6.6.2",
    "knip": "^3.13.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "vue-tsc": "^1.8.27"
  }
}
