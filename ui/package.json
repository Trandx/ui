{
  "name": "@package/ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Nucle-x UI",
  "author": "Trandx-code <trandxcode@gmail.com>",
  "module": "./dist/lib/index.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/lib/style.css",
  "files": [
    "dist/lib",
    "dist/types"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./css": {
      "import": "./dist/lib/style.css"
    }
  },
  "scripts": {
    "types": "vue-tsc",
    "types:watch": "vue-tsc --watch",
    "build": "vue-tsc && vite build",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@package/tailwind-config": "workspace:*",
    "@package/tsconfig": "workspace:*",
    "@trandx/vue-cropper": "workspace:*",
    "@trandx/native": "workspace:*",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "autoprefixer": "catalog:",
    "knip": "catalog:",
    "postcss": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:"
  }
